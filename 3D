
{
    local file=;
    for file in $(export LC_COLLATE=C; echo /etc/profile.d/*.$1);
    do
        [ -e "${file}" ] && . "${file}";
    done;
    if [ -n "${MINGW_MOUNT_POINT}" ]; then
        for file in $(export LC_COLLATE=C; echo ${MINGW_MOUNT_POINT}/etc/profile.d/*.$1);
        do
            [ -e "${file}" ] && . "${file}";
        done;
    fi
}

